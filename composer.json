{
  "name": "bamfura/conteudo-loja",
  "description": "e-commerce system for badaso",
  "keywords": [
    "laravel",
    "admin",
    "panel",
    "dashboard",
    "pwa",
    "vue",
    "generator",
    "badaso",
    "commerce"
  ],
  "license": "proprietary",
  "homepage": "https://badaso.uatech.co.id/",
  "support": {
    "issues": "https://github.com/uasoft-indonesia/badaso-commerce-module/issues",
    "discussion": "https://github.com/uasoft-indonesia/badaso-commerce-module/discussions",
    "source": "https://github.com/uasoft-indonesia/badaso-commerce-module"
  },
  "type": "library",
  "require": {
    "mbezhanov/faker-provider-collection": "^2.0.1"
  },
  "authors": [
    {
      "name": "Bamfura",
      "email": "geral@bamfura.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Uasoft\\Badaso\\Module\\Commerce\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Uasoft\\Badaso\\Tests\\": "tests/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "Uasoft\\Badaso\\Module\\Commerce\\Providers\\BadasoCommerceModuleServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4",
    "bamfura/core": "*",
    "orchestra/testbench": "*"
  }
}
